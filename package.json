{
  "author": "Lucas",
  "name": "typing-mecamatic-testing",
  "version": "1.0.0",
  "description": "A Mecamatic clone",
  "keywords": [],
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@material-ui/styles": "^4.11.4",
    "@xstate/react": "^1.5.1",
    "mobx": "^6.3.2",
    "ramda": "^0.27.1",
    "ramda-adjunct": "^2.33.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "^4.0.3",
    "remove": "^0.1.5",
    "xstate": "^4.23.0",
    "electron-is-dev": "^2.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/runtime": "7.13.8",
    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@xstate/inspect": "^0.4.1",
    "concurrently": "^6.2.0",
    "electron": "^13.1.6",
    "electron-builder": "^22.11.7",
    "typescript": "^4.1.3",
    "wait-on": "^6.0.0",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "electron-dev": "concurrently \"set BROWSER=none && npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder --c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run build",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "mecamatic.typing",
    "productName": "Mecamatic",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extraResources": [
      {
        "from": "./data",
        "to": "data"
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
